const mongoose = require('mongoose');

// Define the schema for the FormatModel
const FormatModelSchema = new mongoose.Schema({
    countryName: String,
    countryCode: String,
    format: String
});

// Define the FormatModel based on the schema
const FormatModel = mongoose.model('FormatModel', FormatModelSchema);

// Export the FormatModel for use in other modules
module.exports = FormatModel;