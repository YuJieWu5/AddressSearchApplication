// models/searchModel.js

const mongoose = require('mongoose');

const defaultAddressSchema = new mongoose.Schema({
    buildingNumber: {Int32Array, "minimum": 0},
    streetInfo: String,
    city: String,
    statePrefectureProvidence: String,
    country: String,
    zipcode: String,
});

const allCountriesSchema = new mongoose.Schema({
    buildingNumber: {Int32Array, "minimum": 0},
    streetInfo: String,
    buildingInstitutionName: String,
    city: String,
    statePrefectureProvidence: String,
    neighborhoodSubdivision: String,
    country: String,
    zipcode: String,
});

const nameSchema = new mongoose.Schema({
    firstname: String,
    lastname: String
});



const searchModel = mongoose.model('searchModel', defaultAddressSchema);

module.exports = searchModel;