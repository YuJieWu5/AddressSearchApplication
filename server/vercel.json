{
 "version": 2,
 "builds": [
  {
   "src": "server.js",
   "use": "@vercel/node",
   "config": { "includeFiles": ["functionalities/**"] }
  }
 ],
 "routes": [
    {
        "src": "/api-docs",
        "dest": "/api/swagger.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/server.js"
      }
 ]
}